CC = /opt/compiler/gcc-4.8.2/bin/gcc
CXX = /opt/compiler/gcc-4.8.2/bin/g++

CFLAGS = -I../../../ -lstdc++

CFLAGS += -DC_SIZE=2048

CFLAGS += -DC_STRING_1=\"\"   

CFLAGS += -DC_STRING_2=\"\"   


memcpy_test : memcpy.o
			  cc $(CFLAGS) -o memcpy_test memcpy.o
memcpy.o : memcpy.cpp
			cc $(CFLAGS) -c memcpy.cpp

memcpy_thread_test : thread.o
			  g++ $(CFLAGS) -o memcpy_thread_test thread.o -pthread -std=c++11 

thread.o : thread.cpp
			g++ $(CFLAGS) -c thread.cpp -pthread -std=c++11 

memcpy_thread_test_string : thread_string.o
			  g++ $(CFLAGS) -o memcpy_thread_test_string thread_string.o -pthread -std=c++11 

thread_string.o : thread_string.cpp
			g++ $(CFLAGS) -c thread_string.cpp -pthread -std=c++11 
		
clean : 
			rm *.o memcpy_test memcpy_thread_test memcpy_thread_test_string


			
